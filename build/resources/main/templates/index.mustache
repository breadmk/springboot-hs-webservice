{{>layout/header}}
    <h1>12/4 집들이 겸 연말 모임 Page</h1>
<div class="col-md-12">
    <div class="row">
        <div class="col-md-6">
            <a href="/posts/save" role="button" class="btn btn-primary">글동록</a>
            {{#realName}}
                Logged in as: <span id="user">{{realName}}</span>
                <a href="/logout" class="btn btn-info active" role="button">Logout</a>
            {{/realName}}
            {{^realName}}
            <a href="/oauth2/authorization/google" class="btn btn-success active" role="button">Google Login</a>
            <a href="/oauth2/authorization/naver" class="btn btn-secondary active" role="button">Naver Login</a>
            {{/realName}}
        </div>
    </div>

    <br>

    <table class="table table-horizontal table-bordered">
        <thead class="thred-strong">
        <tr>
            <th>글번호</th>
            <th>제목</th>
            <th>작성자</th>
        </tr>
        </thead>
        <tbody id="tbody">
            <tr>
                <td>1</td>
                <td><a href="/location/1">오시는길</a></td>
                <td>강민규</td>
            </tr>
            <tr>
                <td>2</td>
                <td><a href="/parking">주차안내</a></td>
                <td>강민규</td>
            </tr>
        </tbody>
    </table>
<!-- 목록 출력 영영 -->
<table class="table table-horizontal table-bordered">
    <thead class="thred-strong">
        <tr>
            <th>게시글번호</th>
            <th>제목</th>
            <th>작성자</th>
            <th>최종작성일</th>
        </tr>
    </thead>
    <tbody id="tbody">
    {{#posts}}
    <tr>
        <td>{{id}}</td>
        <td><a href="/posts/content/{{id}}">{{title}}</a></td>
        <td>{{author}}</td>
        <td>{{modifiedDate}}</td>
    </tr>
    {{/posts}}
    </tbody>
</table>
</div>
{{>layout/footer}}